<body>
    <h1>Reverse A Number</h1>
</body>
<script>
    var num = prompt("Enter a number")
var rev = 0;

while(num)
{
    var d = num % 10;
    rev = rev * 10 + d;
    num=num/10|0;
}
console.log(rev);

</script>
</html>
